{
    "name": "al-one/laravel-huawei-push",
    "description": "Huawei Push For Laravel Notifications",
    "keywords": ["laravel", "illuminate", "notifications", "huawei", "huawei-push", "huawei-notif"],
    "authors": [
        {
            "name": "Alone",
            "email": "hi@anlo.ng"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/contracts": "^5.3 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/notifications": "^5.3 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/support": "^5.3 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/cache": "^5.3 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "opis/closure": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Alone\\LaravelHuaweiPush\\": "src/"
        }
    },
    "extra": {
        "laravel": {
